  "tiers": {
    "free": {
      "description": "Free tier with basic processing",
      "processing_steps": [
        "noise_reduction",
        "eq_adjustment",
        "compression",
        "loudness_normalization"
      ],
      "max_file_size": 50,
      "output_formats": ["mp3"]
    },
    "professional": {
      "description": "Professional tier with advanced processing",
      "processing_steps": [
        "noise_reduction",
        "eq_adjustment",
        "compression",
        "stereo_enhancement",
        "limiting",
        "loudness_normalization"
      ],
      "max_file_size": 100,
      "output_formats": ["wav", "mp3", "flac"]
    },
    "advanced": {
      "description": "Advanced tier with manual controls",
      "processing_steps": [
        "noise_reduction",
        "eq_adjustment",
        "compression",
        "stereo_enhancement",
        "limiting",
        "loudness_normalization"
      ],
      "max_file_size": 200,
      "output_formats": ["wav", "mp3", "flac", "aiff"],
      "manual_controls": true
    }
  },
  "processing_pipeline": {
    "noise_reduction": {
      "algorithm": "spectral_subtraction",
      "sensitivity": 0.7
    },
    "eq_adjustment": {
      "bands": [32, 64, 125, 250, 500, 1000, 2000, 4000, 8000, 16000],
      "q_factor": 1.0
    },
    "compression": {
      "lookahead": 5,
      "knee": 6
    },
    "stereo_enhancement": {
      "width": 1.2,
      "phase_correction": true
    },
    "limiting": {
      "true_peak_limiting": true,
      "oversampling": 4
    },
    "loudness_normalization": {
      "algorithm": "ebu_r128",
      "gating": "relative"
    }
  }
