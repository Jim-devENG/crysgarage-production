{
    "name": "crysgarage/ml-pipeline",
    "description": "Crys Garage ML Pipeline - Standalone PHP Backend",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Crys Garage Team",
            "email": "team@crysgarage.studio"
        }
    ],
    "require": {
        "php": ">=8.0"
    },
    "require-dev": {},
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "echo 'ML Pipeline - Standalone PHP Backend'"
        ]
    }
}